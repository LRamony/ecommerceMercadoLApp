<center><br><br><br>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
   
        <div class="col-md-4 col-md-offset-4">
            <div class="login-panel panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">Por favor, inscreva-se</h3>
                </div>
                <div class="panel-body">
                    <form role="form">
                        <fieldset>
                            <div class="form-group">
                                <%= f.email_field :email, autofocus: true, autocomplete: "email",class:"form-control", placeholder:"E-mail", type:"email" %>
                            </div>
                            <div class="form-group">
                                <%= f.password_field :password, autocomplete: "current-password",class:"form-control", placeholder:"Password", type:"password", value:"" %>
                            </div>
                            <div class="checkbox">
                                <% if devise_mapping.rememberable? %>
                                <div class="field">
                                  <%= f.check_box :remember_me %>
                                  <%= f.label :remember_me %>
                                </div>
                              <% end %>
                            </div>
                            <!-- Change this to a button or input when using this as a form -->
                            <div class="actions">
                              <%= f.submit "Login", class:"btn btn-lg btn-success btn-block" %>
                            </div>
                        </fieldset>
                    </form>
                </div>
            </div>
        </div>
    
<% end %><br>

<%# <%= render "admins/shared/links" %> 

</center>